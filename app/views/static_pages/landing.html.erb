		<footer id="footer">
			<div class="container">
				<div class="row text-center">
					<div class="footer-content">
						<p>Copyright &copy; 2016-2017 Developed By<a href="http://www.dais.com"> DAISocial team</a> </p>
					</div>
				</div>
			</div>
		</footer>
            <!-- Modal -->
  <div class="modal fade bs-example-modal-sm" id="myModal" role="dialog">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Please sign in</h4>
        </div>
        <div class="modal-body">
        <%= form_for(:user, :url => session_path(:user)) do |f| %>
                  <%= render 'layouts/flash' %>
                <div class="form-group">
                  <%= f.label :email %>
                  <%= f.text_field :email,class: "form-control",placeholder: t('home_page.email_label') %>
                </div>
                <div class="form-group">
                  <%= f.label :password %>
                  <%= f.password_field :password,class: "form-control",placeholder: t('home_page.password_label') %>
                </div>
                <div class="form-group">
                  <%= f.submit t('home_page.submit') ,class: "btn btn-blue btn-effect" %>
                </div>
            <!--<div class="form-group">
                	<input class="form-control" type="hidden" name="user[email]" id="user_email" value="123">
                </div> -->
        <% end %>
        </div>
      </div>
    </div>
  </div>
